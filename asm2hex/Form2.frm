Attribute VB_Name = "Form2"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Dim i As Integer
Dim ser() As String
Dim vcode() As String
Dim lne As Long
Dim j As Long
Dim has As Long
Dim addr As String
Const fort = 16

Private Sub Form_Load()
addr = Form1.Text2.Text
mid_Form Me

Text1(0).Top = 0
Text1(0).Left = 500

Text2(0).Top = 0
Text2(0).Left = 0
Text2(0).Width = 500
Text2(0).Height = Text1(0).Height

For i = 1 To fort
Text1(i).Height = Text1(0).Height
Text1(i).Left = Text1(0).Left
Text1(i).Top = Text1(i - 1).Top + Text1(i - 1).Height

Text2(i).Top = Text1(i).Top
Text2(i).Left = 0
Text2(i).Width = 500
Text2(i).Height = Text1(i).Height
Next i
For i = fort To 25
Text1(i).Visible = False
Text2(i).Visible = False
Next i

For i = 0 To 25
Text2(i).Enabled = False
Next i

lne = txtline(addr) - 1
ReDim vcode(0 To lne) As String
ReDim ser(0 To lne) As String
j = 0
has = 0
Open addr For Input As #1 ' 打开文件。
    
    Do While Not EOF(1) ' 循环至文件尾。
      Line Input #1, vcode(j)
      Debug.Print vcode(j)
    If (vcode(j) = "") Then
    ser(j) = ""
    ElseIf (Mid(vcode(j), 1, 2) = "//") Then
    ser(j) = ""
    Else
    ser(j) = vtoh(has, 4)
    has = has + 1
    End If
    j = j + 1
    Loop
    
Close #1 ' 关闭文件。

For i = 0 To fort

If (i <= lne) Then
Text2(i) = ser(i)
Text1(i) = vcode(i)
Else
Text2(i) = ""
Text1(i) = ""
End If

Next i


VScroll1.Min = 0
VScroll1.Max = lne






End Sub





Private Sub Save_Click()
  Open addr For Output As #1 ' 打开输出文件。
    For j = 0 To lne
    Print #1, vcode(j)
    Next j
  Close #1
  
End Sub

Private Sub Text1_Change(Index As Integer)

Dim cgh As Long
cgh = VScroll1.Value + Index

If (cgh <= lne) Then
 vcode(cgh) = Text1(Index)
End If

End Sub

Private Sub VScroll1_Change()
Dim kv As Long
For i = 0 To fort
kv = VScroll1.Value + i
If (kv <= lne) Then
Text2(i) = ser(kv)
Text1(i) = vcode(kv)
Else
Text2(i) = ""
Text1(i) = ""
End If

Next i
End Sub
